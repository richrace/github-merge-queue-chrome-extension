{
  "manifest_version": 3,
  "name": "Github Merge Queue",
  "version": "0.1",
  "description": "Adds a link to see the active merge queue",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["main.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
